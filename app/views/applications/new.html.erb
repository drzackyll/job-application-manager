<%= form_for @application do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <%= f.label :organization %>
  <%= f.text_field :organization %><br>
  <%= f.label :hiring_manager %>
  <%= f.text_field :hiring_manager %><br>
  <%= f.label :email %>
  <%= f.text_field :email %><br>
  <%= f.label :url %>
  <%= f.text_field :url %><br>
  <%= f.label :description %>
  <%= f.text_field :description %><br>
  <%= f.submit %>
<% end %>
